{
  "compilerOptions": {
    "target": "es2020",
    "module": "node16",
    "strict": true,
    "preserveConstEnums": true,
    "noEmit": true,
    "noEmitHelpers": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "forceConsistentCasingInFileNames": true
  }
}
